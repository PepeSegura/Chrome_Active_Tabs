{
    "manifest_version": 2,
    "name": "Tab URL Extractor",
    "version": "1.0",
    "description": "Get URLs of all open tabs",
    "permissions": [
      "tabs"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
  